Config { font = "-*-terminus-medium-*-*-*-16-*-*-*-*-*-iso8859-*"
       , bgColor = "#002b36"
       , fgColor = "#839496"
       , position = TopW L 90
       , commands = [ Run Weather "EGPF"
                        [
                         "-t"
                         ," <tempF>F"
                         ,"-L"
                         ,"64"
                         ,"-H"
                         ,"77"
                         ,"--normal","#859900"
                         ,"--high","#dc322f"
                         ,"--low","#268bd2"
                        ] 36000
                    , Run Cpu ["-L","3","-H","50","--normal","#859900","--high","#dc322f"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
                    , Run Date "%a %b %_d %l:%M" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %cpu% | %memory% * %swap%    <fc=#ee9a00>%date%</fc> | %EGPF%"
       }
