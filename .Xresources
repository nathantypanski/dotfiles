!##########################
! solarized color scheme  #
!##########################
!URxvt.depth: 32

*background: #002b36
*foreground: #657b83
!!*fading: 40
*fadeColor: #002b36
*cursorColor: #93a1a1
*pointerColorBackground: #586e75
*pointerColorForeground: #93a1a1

!! black dark/light
*color0: #073642
*color8: #002b36

!! red dark/light
*color1: #dc322f
*color9: #cb4b16

!! green dark/light
*color2: #859900
*color10: #586e75

!! yellow dark/light
*color3: #b58900
*color11: #657b83

!! blue dark/light
*color4: #268bd2
*color12: #839496

!! magenta dark/light
*color5: #d33682
*color13: #6c71c4

!! cyan dark/light
*color6: #2aa198
*color14: #93a1a1

!! white dark/light
*color7: #eee8d5
*color15: #fdf6e3

!! Font for everything
*font: -*-terminus-medium-r-normal-*-16-*-*-*-*-*-iso10646-*
URxvt*italicFont: -*-terminus-medium-r-normal-*-16-*-*-*-*-*-iso10646-*
URxvt*boldItalicFont: -*-terminus-bold-r-normal-*-16-*-*-*-*-*-iso10646-*

!! Cursor size
Xcursor.size: 16

!! xft
Xft.dpi:        131
Xft.antialias:  true

!! urxvt
URxvt*termName: rxvt-unicode
URxvt*imLocale: en_Us.utf8
URxvt*depth:                  16
URxvt*scrollstyle:            plain
URxvt*scrollBar:              False
URxvt*scrollBar_right:        True
URxvt*scrollBar_align:        top
! Makes urxvt invisible to window managers. Or not when its false.
URxvt*override-redirect:      False
URxvt*geometry:               80x25
URxvt*internalBorder:         0
URxvt*externalBorder:         0
URxvt*lineSpace:              0
URxvt*borderLess:             False
URxvt.intensityStyles:        false
! Dont know what this does:
URxvt*preeditType:            OverTheSpot
URxvt*urlLauncher:            firefox
URxvt*cursorBlink:            True
URxvt*pointerBlank:           True
URxvt*savelines:              65535
URxvt*scrollttykeypress:      true
URxvt.iconFile:               /usr/share/icons/oxygen/32x32/apps/utilities-terminal.svg
! Colored man pages and other highlighted stuff
URxvt.colorIT:                #268bd2
URxvt.colorBD:                #dc322f
URxvt.colorUL:                #859900
! URxvt*print-pipe:       cat > $(echo urxvt.dump.`date +'%Y%M%d%H%m%S'`)

!! dzen2

dzen2.font:                   -*-terminus-medium-r-*-*-18-*-*-*-*-*-*-*
dzen2.foreground:             #fdf6e3
dzen2.background:             #002b36
